# API Keys
GEMINI_API_KEY=your_gemini_api_key_here
XAI_API_KEY=your_xai_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
PORT=3458

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# RAG Configuration
RAG_SIMILARITY_THRESHOLD=0.3
USE_EDGE_FUNCTIONS=true

# Resend Email Configuration
RESEND_API_KEY=your_resend_api_key_here
CONTACT_EMAIL=contact@yourdomain.com
RESEND_FROM_EMAIL=noreply@yourdomain.com
RESEND_FROM_NAME=Your App Name
RESEND_VERIFIED_EMAIL=verified@yourdomain.com
PUBLIC_BASE_URL=https://www.yourdomain.com

# Debug Mode (optional)
# Set to 'true' to enable debug logging in production
# By default, debug logs only appear in development (NODE_ENV=development)
# DEBUG=true

# Frontend Debug Mode (optional)
# Set to 'true' to enable frontend debug logging in production
# By default, frontend debug logs only appear in development mode
# VITE_DEBUG=true



