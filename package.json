{
  "name": "pdf-chatbot",
  "version": "1.0.0",
  "description": "PDF-based chatbot using Gemini 2.5",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:app": "cd app-src && vite",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:app\"",
    "test:dist": "npm run build && cd dist && node server.js",
    "start:dist": "cd dist && node server.js",
    "build": "npm run build:app && node build.js",
    "build:app": "cd app-src && vite build",
    "embed": "node scripts/chunk-and-embed.js",
    "embed:local": "node scripts/chunk-and-embed-local.js",
    "compare": "node scripts/compare-embeddings.js"
  },
  "keywords": [
    "chatbot",
    "pdf",
    "gemini",
    "ai"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@supabase/auth-helpers-react": "^0.5.0",
    "@supabase/supabase-js": "^2.76.1",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.0.4",
    "@xenova/transformers": "^2.17.2",
    "busboy": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.7.0",
    "openai": "^4.73.0",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "pg": "^8.16.3",
    "react": "^19.2.0",
    "react-contenteditable": "^3.3.7",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.4",
    "typescript": "^5.9.3",
    "uuid": "^9.0.1",
    "vite": "^7.1.11"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.0",
    "nodemon": "^3.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.0"
  }
}
